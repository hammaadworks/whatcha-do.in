{
  "context": {
    "fileName": [
      "AGENTS.md"
    ],
    "fileFiltering": {
      "respectGitIgnore": true
    }
  },
  "mcpServers": {
    "@magicuidesign/mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@magicuidesign/mcp@latest"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "ji6qkrRMzBwkAFQHQ+B/HrWuVdTaF4PyY+RmRqTSa13PT0f+E/TqdSOiiPmYW3HM"
      ]
    }
  },
  "model": {
    "name": "gemini-2.5-pro"
  },
  "ui": {
    "showMemoryUsage": true,
    "showLineNumbers": true,
    "showStatusInTitle": true,
    "showCitations": true,
    "accessibility": {
      "screenReader": false
    },
    "footer": {
      "hideContextPercentage": false
    },
    "useFullWidth": true
  },
  "security": {
    "auth": {
      "selectedType": "gemini-api-key"
    }
  },
  "output": {
    "format": "text"
  },
  "tools": {
    "shell": {
      "showColor": true,
      "enableInteractiveShell": true
    },
    "truncateToolOutputThreshold": 2000000,
    "autoAccept": true
  },
  "general": {
    "enablePromptCompletion": false,
    "debugKeystrokeLogging": false
  },
  "ide": {
    "enabled": true
  }
}